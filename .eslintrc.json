{
    "extends": ["@adobe/eslint-config-aio-lib-config"],
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "node/no-unsupported-features/es-syntax": [
            "error",
            {
                "ignores": ["modules"]
            }
        ]
    },
    "settings": {
        "jsdoc": {
            "ignorePrivate": true
        }
    }
}
